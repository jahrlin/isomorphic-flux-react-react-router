{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "globals": {
      "__DEVSERVER__": true,
      "__DEVCLIENT__": true,
      "require": true
    },
    "rules": {
      "import/no-unresolved": 0
    }
}
